<div class="jumbotron">
  <h1>Workout Lounge!</h1>
  <p>Track your workouts and share them with the world.</p>
</div>

<div class="row">
  <div class="col-md-6">
    <h3 class="text-center">List of Members</h3>
    <br>
    <ul>
      <% @users.each do |user| %>
        <li class="well text-center"><%= user.full_name %></li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-6">
    <h3 class="text-center">Recent Workouts</h3>
    <br>
    <ul>
      <% @workouts.each do |workout| %>
        <li class="well text-center"><%= "#{workout.name}, #{workout.workout_date.strftime("%A %B %d")}" %></li>
      <% end %>
    </ul>
  </div>
</div>